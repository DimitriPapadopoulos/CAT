detail auth_log_%(deploymentid)s-%(instid)s {
	filename = ${radacctdir}/detail-%(deploymentid)s-%(instid)s-%(format)s
	permissions = 0600
	suppress {
		User-Password
	}
}
detail reply_log_%(deploymentid)s-%(instid)s {
	filename = ${radacctdir}/detail-%(deploymentid)s-%(instid)s-%(format)s
	permissions = 0600
}
detail pre_proxy_log_%(deploymentid)s-%(instid)s {
	filename = ${radacctdir}/detail-%(deploymentid)s-%(instid)s-%(format)s
	permissions = 0600
	# You may also strip out passwords completely
	#suppress {
		# User-Password
	#}
}
detail post_proxy_log_%(deploymentid)s-%(instid)s {
	filename = ${radacctdir}/detail-%(deploymentid)s-%(instid)s-%(format)s
	permissions = 0600
}
